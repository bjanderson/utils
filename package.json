{
  "name": "@bjanderson/utils",
  "version": "1.8.0",
  "license": "MIT",
  "description": "JavaScript utilities for writing clean code.",
  "author": "BJ Anderson",
  "publishConfig": {
    "@bjanderson:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bjanderson/utils.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "fix-ts-paths": "node scripts/fix-ts-paths.js",
    "doc": "typedoc --out ./docs/utils src/**/*.ts",
    "lint": "eslint ./src --ext .ts",
    "prepublish": "npm run build",
    "push:major": "npm version major",
    "push:minor": "npm version minor",
    "push:patch": "npm version patch",
    "start": "npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --clearCache && jest --coverage --watch"
  },
  "peerDependencies": {
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.29.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.4.1",
    "eslint-plugin-json": "3.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "import": "0.0.6",
    "jest": "26.6.3",
    "moment": "2.29.1",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "tslint": "5.20.1",
    "typedoc": "0.21.2",
    "typescript": "4.3.4"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
